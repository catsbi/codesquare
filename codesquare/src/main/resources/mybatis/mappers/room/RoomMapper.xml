<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bit.codesquare.mapper.room.RoomMapper">

	<select id="getAllRoom" resultType="Room">
		select * from codesquare.RoomInfo
	</select>
	
	<select id="getid" resultType="Room">
		SELECT * FROM codesquare.RoomInfo where CompanyId=#{companyid}
	</select>
	
	<select id="getroom" resultType="Room">
		select * from codesquare.RoomInfo where id=#{id}
	</select>
	
</mapper>